{
  "name": "next-goat",
  "description": "Next.js starter made for goats 🐐",
  "private": true,
  "version": "0.1.0",
  "author": {
    "name": "Philip Johnston",
    "email": "tunderadev@gmail.com",
    "url": "https://github.com/tundera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/next-goat.git"
  },
  "engines": {
    "node": ">=12"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "NODE_OPTIONS='--inspect' next",
    "generate:graphql": "graphql-codegen",
    "generate:groq": "sanity-codegen",
    "generate": "yarn generate:graphql && yarn generate:groq",
    "build:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/services/nexus/schema",
    "build:next": "next build",
    "build": "yarn build:nexus && yarn build:next",
    "db:introspect": "prisma introspect --schema='db/schema.prisma' --print",
    "db:format": "prisma format",
    "db:push": "prisma db push --preview-feature --schema='db/schema.prisma'",
    "db:studio": "prisma studio",
    "db:migrate:dev": "prisma migrate dev --schema='db/schema.prisma' --preview-feature",
    "db:migrate:reset": "prisma migrate reset --schema='db/schema.prisma' --preview-feature",
    "db:migrate:deploy": "prisma migrate deploy --schema='db/schema.prisma' --preview-feature",
    "db:migrate:status": "prisma migrate status --schema='db/schema.prisma' --preview-feature",
    "studio:start": "yarn workspace studio start",
    "studio:deploy": "yarn workspace studio deploy",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --runInBand",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:generate-output": "jest --passWithNoTests --json --outputFile=.jest-test-results.json || true",
    "lint": "eslint './{src,pages.storybook}/**/*.{js,jsx,ts,tsx}'",
    "fix": "eslint './{src,pages,.storybook}/**/*.{js,jsx,ts,tsx}' --fix",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build:storybook": "build-storybook && yarn storybook extract",
    "forms:deploy": "formspree deploy -k $FORMSPREE_DEPLOY_KEY"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.3",
    "@chakra-ui/react": "^1.1.6",
    "@chakra-ui/theme-tools": "^1.0.3",
    "@emotion/is-prop-valid": "^1.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@formspree/react": "^2.2.3",
    "@mdx-js/mdx": "^2.0.0-next.8",
    "@mdx-js/react": "^2.0.0-next.8",
    "@mdx-js/tag": "^1.0.0-alpha.6",
    "@prisma/client": "^2.15.0",
    "@storybook/react": "^6.1.15",
    "a11y-react-emoji": "^1.1.2",
    "apollo-server-micro": "^2.19.2",
    "babel-plugin-superjson-next": "^0.2.1",
    "change-case": "^4.1.2",
    "date-fns": "^2.16.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "favicons": "^6.2.0",
    "framer-motion": "^3.2.2-rc.1",
    "globby": "^11.0.2",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.2",
    "graphql-request": "^3.4.0",
    "graphql-shield": "^7.5.0",
    "graphql-tag": "^2.11.0",
    "gray-matter": "^4.0.2",
    "icomoon-react": "^2.0.19",
    "image-size": "^0.9.3",
    "light-date": "^1.2.0",
    "mdx-prism": "^0.3.1",
    "mdx-utils": "^0.2.0",
    "next": "^10.0.6",
    "next-mdx-remote": "^2.1.3",
    "next-nprogress-emotion": "^2.0.0",
    "next-sanity": "^0.1.8",
    "next-seo": "^4.18.0",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.29.0",
    "prettier": "^2.2.1",
    "prism-react-renderer": "^1.1.1",
    "react": "^0.0.0-experimental-27659559e",
    "react-dom": "^0.0.0-experimental-27659559e",
    "react-dom-confetti": "^0.2.0",
    "react-error-boundary": "^3.1.0",
    "react-hook-form": "^6.14.2",
    "react-icons": "^4.1.0",
    "react-live": "^2.2.3",
    "react-markdown": "^5.0.3",
    "react-query": "^3.6.0",
    "react-svg": "^11.2.3",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^5.0.1",
    "remark-code-titles": "^0.1.1",
    "remark-external-links": "^8.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-parse": "^9.0.0",
    "remark-slug": "^6.0.0",
    "remark-stringify": "^9.0.1",
    "remark-toc": "^7.0.0",
    "sanity-codegen": "^0.8.3",
    "superjson": "^1.5.2",
    "timeago-react": "^3.0.1",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-graphql-request": "3.0.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@prisma/cli": "^2.15.0",
    "@storybook/addon-a11y": "^6.1.15",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-jest": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/addon-storysource": "^6.1.15",
    "@storybook/addons": "^6.1.15",
    "@storybook/theming": "^6.1.15",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-addons-test-utils": "^0.14.25",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "postinstall-postinstall": "^2.1.0",
    "pretty-quick": "^3.1.0",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^3.0.2",
    "storybook-addon-designs": "^5.4.3",
    "storybook-addon-next-router": "^2.0.3",
    "storybook-addon-performance": "^0.14.0",
    "storybook-mobile": "^0.1.29",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "{src,pages.storybook}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn lint && yarn test"
    }
  }
}
