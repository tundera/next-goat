{
  "name": "tunderadev",
  "description": "tundera.dev website",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "Philip Johnston",
    "email": "tunderadev@gmail.com",
    "url": "https://github.com/tundera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/tunderadev.git"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "scripts": {
    "postinstall": "yarn generate:theme-typings && prisma generate && husky install",
    "dev": "docker-compose up -d && NODE_OPTIONS='--inspect' next",
    "start": "docker-compose up -d && next start",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --runInBand",
    "test:coverage": "jest --passWithNoTests --coverage",
    "test:generate-output": "jest --passWithNoTests --json --outputFile=.jest-test-results.json || true",
    "generate:theme-typings": "chakra-cli tokens styles/theme/index.ts",
    "generate:page:ssr": "next-codegen page --ssr",
    "generate:page:ssg": "next-codegen page --ssg",
    "generate:api": "next-codegen api",
    "codegen:graphql": "graphql-codegen",
    "codegen": "yarn codegen:graphql",
    "build:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only services/graphql/schema",
    "build:next": "yarn db:migrate:deploy && next build",
    "prebuild": "yarn build:nexus && yarn codegen",
    "build": "NODE_ENV=production yarn build:next",
    "db:introspect": "prisma introspect --schema='db/schema.prisma' --print",
    "db:format": "prisma format",
    "db:push": "prisma db push --preview-feature --schema='db/schema.prisma'",
    "db:studio": "prisma studio",
    "db:migrate:dev": "prisma migrate dev --schema='db/schema.prisma' --preview-feature",
    "db:migrate:reset": "prisma migrate reset --schema='db/schema.prisma' --preview-feature",
    "db:migrate:deploy": "prisma migrate deploy --schema='db/schema.prisma' --preview-feature",
    "db:migrate:status": "prisma migrate status --schema='db/schema.prisma' --preview-feature",
    "g:cell": "hygen cell new --name",
    "g:component": "hygen component new --name",
    "g:graphql": "hygen graphql new --name",
    "g:page:basic": "hygen page basic:new --name",
    "g:page:ssr": "hygen page ssr:new --name",
    "g:page:ssg": "hygen page ssg:new --name",
    "g:migration": "yarn -s prisma migrate save --experimental",
    "g:test:component": "hygen test component --name",
    "g:test:factory": "hygen test factory --name",
    "g:test:request": "hygen test request --name",
    "g:test:util": "hygen test util --name",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "lint": "eslint './{src,pages.storybook}/**/*.{js,jsx,ts,tsx}'",
    "fix": "eslint './{src,pages,.storybook}/**/*.{js,jsx,ts,tsx}' --fix",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build:storybook": "build-storybook && yarn storybook extract",
    "forms:deploy": "formspree deploy -k $FORMSPREE_DEPLOY_KEY"
  },
