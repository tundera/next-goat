{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "env": {
    "SANITY_API_TOKEN": "@tunderadev_sanity_api_token",
    "SANITY_API_PREVIEW_SECRET": "@tunderadev_sanity_preview_secret",
    "DATABASE_URL": "@tunderadev_database_url",
    "FORMSPREE_MAILCHIMP_API_KEY": "@tunderadev_formspree_mailchimp_api_key",
    "STRIPE_SECRET_KEY": "@tunderadev_stripe_secret_key",
    "GITHUB_CLIENT_ID": "@tunderadev_github_client_id",
    "GITHUB_CLIENT_SECRET": "@tunderadev_github_client_secret",
    "SMTP_HOST": "@tunderadev_smtp_host",
    "SMTP_PORT": "@tunderadev_smtp_port",
    "SMTP_USER": "@tunderadev_smtp_user",
    "SMTP_PASSWORD": "@tunderadev_smtp_password",
    "PRISMA_SECRET": "@tunderadev_prisma_secret"
  },
  "build": {
    "env": {
      "SANITY_API_TOKEN": "@tunderadev_sanity_api_token",
      "SANITY_API_PREVIEW_SECRET": "@tunderadev_sanity_preview_secret",
      "DATABASE_URL": "@tunderadev_database_url",
      "STRIPE_SECRET_KEY": "@tunderadev_stripe_secret_key",
      "SESSION_SECRET_KEY": "@tunderadev_session_secret_key",
      "GITHUB_CLIENT_ID": "@tunderadev_github_client_id",
      "GITHUB_CLIENT_SECRET": "@tunderadev_github_client_secret",
      "PRISMA_SECRET": "@tunderadev_prisma_secret"
    }
  },
  "redirects": [
    {
      "source": "/metrics",
      "destination": "https://metrics.tundera.dev"
    }
  ]
}
