{
  "version": 2,
  "env": {
    "NEXT_PUBLIC_SITE_NAME": "Next Goat",
    "NEXT_PUBLIC_SITE_URL": "https://next-goat.vercel.app",
    "NEXT_PUBLIC_GRAPHQL_ENDPOINT": "https://next-goat.vercel.app/api/graphql",

    "NEXT_DATABASE_URL": "@next_goat_database_url",
    "NEXT_DATOCMS_API_TOKEN": "@next_goat_datocms_api_token",
    "NEXT_DATOCMS_PREVIEW_SECRET": "@next_goat_datocms_preview_secret"
  },
  "build": {
    "env": {
      "NEXT_POSTGRES_USER": "@next_goat_postgres_user",
      "NEXT_POSTGRES_DB": "@next_goat_postgres_db",
      "NEXT_POSTGRES_PASSWORD": "@next_goat_postgres_password",
      "NEXT_DATABASE_URL": "@next_goat_database_url",
      "NEXT_DATOCMS_API_TOKEN": "@next_goat_datocms_api_token"
    }
  }
}
